{
  "name": "fuzzytree",
  "version": "0.0.2",
  "description": "Fuzzy tree structure with non-deterministic nodes paths",
  "main": "dist/FuzzyTree.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "./node_modules/.bin/mocha --opts tests/mocha.opts tests",
    "build": "./node_modules/.bin/babel --modules umd src/FuzzyTree.js --out-file dist/FuzzyTree.js --source-maps inline",
    "lint": "./node_modules/.bin/jshint src/**.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/EyalAr/FuzzyTree.git"
  },
  "keywords": [
    "tree",
    "node",
    "path",
    "pattern",
    "fuzzy"
  ],
  "author": "Eyal Arubas <EyalArubas@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/EyalAr/FuzzyTree/issues"
  },
  "homepage": "https://github.com/EyalAr/FuzzyTree",
  "dependencies": {
    "lodash": "^3.10.0"
  },
  "devDependencies": {
    "babel": "^5.6.14",
    "jshint": "^2.8.0",
    "mocha": "^2.2.5",
    "should": "^7.0.1"
  }
}
